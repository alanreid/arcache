{

  "arcache": {
    "keyDriver": "memcached",
    "valueDriver": "localDisk",
    "logLevel": "error"
  },

  "memcached": {
    "hosts": [ "127.0.0.1:11211" ],
    "options": {
      "maxKeySize": 250,
      "maxExpiration": 2592000,
      "maxValue": 1048576,
      "poolSize": 10,
      "algorithm": "md5",
      "reconnect": 18000000,
      "timeout": 5000,
      "retries": 5,
      "failures": 5,
      "retry": 30000,
      "remove": false,
      "failOverServers": null,
      "keyCompression": true,
      "idle": 100
    },
    "keyLifetime": 10
  },

  "localDisk": {
    "basePath": "~/"
  }

}